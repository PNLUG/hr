<odoo>
    <!-- Inherit vehicle form view to add new field profile service -->
    <record id="vehicle_form_inherit_profile" model="ir.ui.view" >
        <field name="name">fleet.vehicle</field>
        <field name="model">fleet.vehicle</field>
        <field name="inherit_id" ref="fleet.fleet_vehicle_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='vehicle_category_id']" position="after">
                <field name="profile_id"/>
            </xpath>
        </field>
    </record>
</odoo>
